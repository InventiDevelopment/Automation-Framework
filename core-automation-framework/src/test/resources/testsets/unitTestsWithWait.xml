    </test>
    <test name="WebObject Tests">
        <classes>
            <class name="cz.inventi.qa.framework.tests.unit.web.webobjects.WebApplicationObjectWebTest"/>
            <class name="cz.inventi.qa.framework.tests.unit.web.webobjects.ElementTests"/>
            <class name="cz.inventi.qa.framework.tests.unit.web.webobjects.ComponentTests"/>
            <class name="cz.inventi.qa.framework.tests.unit.web.webobjects.ComponentListTests"/>
            <class name="cz.inventi.qa.framework.tests.unit.web.webobjects.PageTests"/>
